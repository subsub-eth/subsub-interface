{
  "name": "createz",
  "version": "0.1.0",
  "description": "A creator contribution system",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "solidty",
    "truffle"
  ],
  "author": "suiiii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiiii-rip/createz.git"
  },
  "bugs": {
    "url": "https://github.com/suiiii-rip/createz/issues"
  },
  "homepage": "https://createz.xyz",
  "scripts": {
    "build": "NODE_ENV=production npm run clean-dist && webpack --config=app/configs/webpack/prod.js",
    "generate-types": "typechain --target=truffle-v5 'build/contracts/**.json'",
    "generate-frontend-types": "typechain --target=web3-v1 'build/contracts/**.json'",
    "truffle-compile": "truffle compile && npm run generate-types && npm run generate-frontend-types",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './app/src/**/*.{js,ts,tsx}' --quiet",
    "start": "NODE_ENV=development npm run start-dev",
    "start-dev": "webpack serve --config=app/configs/webpack/dev.js",
    "start-prod": "NODE_ENV=production npm run build && node express.js",
    "test": "NODE_ENV=test nyc --reporter=text ts-mocha --require ./app/tests/setup.js --require ignore-styles ./app/tests/**/*.test.{js,jsx,ts,tsx} --exit",
    "test-watch": "NODE_ENV=test nyc --reporter=text ts-mocha --require ./app/tests/setup.js --require ignore-styles ./app/tests/**/*.test.{js,jsx,ts,tsx} --exit --watch --watch-files ./app/tests/**",
    "truffle-test": "truffle test"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@typechain/truffle-v5": "^5.0.0",
    "@typechain/web3-v1": "^3.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.21",
    "@types/enzyme": "^3.10.9",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.12",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.3.4",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "global-jsdom": "^8.1.0",
    "html-webpack-plugin": "^5.3.2",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^7.0.1",
    "jsdom": "^16.7.0",
    "mocha": "^8.2.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^6.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "solidity-linked-list": "^5.0.0",
    "style-loader": "^3.2.1",
    "truffle": "^5.4.5",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5",
    "webpack": "5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fontsource/noto-sans": "^4.5.0",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.2",
    "react-async": "^10.0.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "recoil": "^0.4.0",
    "styled-components": "^5.3.0",
    "styled-reset": "^4.3.4",
    "web3": "^1.5.1"
  }
}
